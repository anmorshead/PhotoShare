
@model List<PhotoShare.Models.Photo> 
@{
    ViewData["Title"] = "Home Page";
}

<h1>All Photos</h1>


@foreach (var item in Model)
{
    <h2>@item.Title</h2>
    <p>
        @item.PhotoId<br/>
        @item.Description<br/>
        @item.CreatedAt<br/>
        @item.ImageFilename<br/>
        <a asp-controller="Home" asp-action="DisplayPhoto" asp-route-id="@item.PhotoId">Display Photo</a>
    </p>

}
